<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.htc.eleven.multimediatesttool">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailResultActivity"
            android:label="DetailResultActivity" />
        <activity
            android:name=".PlaybackTestActivity"
            android:label="PlaybackTestActivity" />
        <activity
            android:name=".RecordingTestActivity"
            android:label="RecordingTestActivity" />
        <activity
            android:name=".VoiceCallTestActivity"
            android:label="VoiceCallTestActivity" />
        <activity
            android:name=".VoipCallActivity"
            android:label="VoipCallActivity" />
        <activity android:name=".NDKDemoActivity" />
        <activity android:name=".VideoTestActivity" />
        <activity android:name=".VideoRecordingActivity" />
    </application>

</manifest>